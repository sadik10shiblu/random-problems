
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,q;
    cin>>n>>q;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
     for(int i=0;i<q;i++)
     {
         int x;
         cin>>x;
         int flag=0;
         for(int i=0;i<n;i++)
         {
             if(a[i]==x)
             {
                 flag=1;
             }
         }
             if(flag==1)
             {
                 cout<<"found";
             }
             else
             {
                 cout<<"not found";
             }
     }
     return 0;
}

in this way gonna face TLE to avoid that need to use:
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int N,Q;

    cin>>N>>Q;
        int A[N];
    for(int i=0;i<N;i++)
    {
        cin>>A[i];
    }
    sort(A,A+N);
    while(Q--)
    {
        int X;
        cin>>X;
        int l=0,r=N-1;
        int flag=0;
        while(l<=r)
        {
            int mid=(l+r)/2;
            if(A[mid]==X)
            {
                flag=1;
                break;
            }
            else if(A[mid]>X)
            {
                r=mid-1;
            }
            else if(A[mid]<X)
            {
                l=mid+1;
            }
        }
        if(flag==1)
        {
            cout<<"found\n";
        }
        else
        {
            cout<<"not found\n";
        }
    }
    return 0;
}

