
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,q;
    cin>>n>>q;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
     for(int i=0;i<q;i++)
     {
         int x;
         cin>>x;
         int flag=0;
         for(int i=0;i<n;i++)
         {
             if(a[i]==x)
             {
                 flag=1;
             }
         }
             if(flag==1)
             {
                 cout<<"found";
             }
             else
             {
                 cout<<"not found";
             }
     }
     return 0;
}

in this way gonna face TLE to avoid that need to use:

#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int v;
    cin>>v;
    int flag=0;
    int l=0,r=n-1;
    while(l<=r)
    {

        int mid=(r+l)/2;
        if(a[mid]==v)
        {
            flag=1;
            break;
        }
        else if(a[mid]>v)
        {
            r= mid-1;
        }
        else if(a[mid]<v)
        {
            l= mid+1;
        }
    }
    if(flag==1)
    {
        cout<<"found";
    }
    else
    {
        cout<<"Not found";
    }
}

